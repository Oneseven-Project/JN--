/* widget/countdown/countdown.wxss */
.count-down-timer{
  background: #fff;
  font-size: 24rpx;
  color: #332500;
  box-sizing: border-box;
}
.count-down{
  display: inline-block; 
  background: #332500;
  font-size: 24rpx;
  color: #fff;
  border-radius: 4rpx;
  width: 34rpx;
  height: 40rpx;
  line-height: 40rpx;
  text-align: center;
}
.count-down-point{
  padding: 0 5rpx;
  color: #999;
  font-size: 24rpx;
}